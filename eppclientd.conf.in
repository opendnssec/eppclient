<!-- $Id$ -->

<eppclient>
 <database>@EPPCLIENTD_DB@</database>
 <pipe>@EPPCLIENTD_PIPE@</pipe>
 <pidfile>@EPPCLIENTD_PID@</pidfile>
 <ackcommand>echo %s</ackcommand>
 <registry>
   <suffix>.se</suffix>
   <host>epptest.iis.se</host>
   <port>700</port>
   <clID>user</clID>
   <pw>password</pw>
   <clientcert>
     <file>mycert.pem</file>
     <type>PEM</type>
   </clientcert>
   <clientkey>
     <file>mykey.pem</file>
     <type>PEM</type>
     <password>foo</password>
   </clientkey>
   <svcExtension>
<![CDATA[
     <extURI>urn:se:iis:xml:epp:iis-1.1</extURI>
]]>
   </svcExtension>
   <maxrate>360</maxrate>
   <expirytime>3600</expirytime>
 </registry>
 <registry>
   <suffix>.foo</suffix>
   <host>epptest.foo</host>
   <port>700</port>
   <clID>user</clID>
   <pw>password</pw>
   <svcExtension>
<![CDATA[
     <extURI>look:at:my:cute:uri</extURI>
     <extURI>and:a:second:one</extURI>
]]>
   </svcExtension>
 </registry>
</eppclient>
